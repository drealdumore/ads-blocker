# Server Configuration
PORT=3000
NODE_ENV=development

# CORS Configuration
ALLOWED_ORIGINS=*

# Logging
LOG_LEVEL=info

# Security
RATE_LIMIT_WINDOW_MS=900000
RATE_LIMIT_MAX_REQUESTS=1000

# Blocklist Update Schedule (in hours)
BLOCKLIST_UPDATE_INTERVAL=24

# Proxy Configuration
PROXY_TIMEOUT=30000
PROXY_ENABLE_LOGS=true
